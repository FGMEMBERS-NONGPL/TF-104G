 <!-- Suppress Seat/Pilot Models in Cockpit View -->
 <animation>
  <type>select</type>
  <object-name>PilotFwd</object-name>
  <object-name>SeatFwd</object-name>
  <condition>
   <not>
   <equals>
    <property>/sim/current-view/view-number</property>
    <value>0</value>
   </equals>
   </not>
  </condition>
 </animation>

