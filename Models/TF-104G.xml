 <!-- Landing Gear and Doors -->

 <animation>
  <type>select</type>
  <object-name>NoseGear</object-name>
  <object-name>NoseTire</object-name>
  <object-name>NGearDoorRight</object-name>
  <object-name>NGearDoorLeft</object-name>
  <object-name>LMainGear</object-name>
  <object-name>LMainTire</object-name>
  <object-name>LMainGearDoors</object-name>
  <object-name>RMainGear</object-name>
  <object-name>RMainTire</object-name>
  <object-name>RMainGearDoors</object-name>
  <condition>
   <greater-than>
    <property>/gear/gear[0]/position-norm</property>
    <value>0.1</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NoseGear</object-name>
  <object-name>NoseTire</object-name>
  <property>/gear/gear/position-norm</property>
  <interpolation>
    <entry><ind>0</ind><dep>90</dep></entry>
    <entry><ind>0.1</ind><dep>0</dep></entry>
    <entry><ind>1</ind><dep>0</dep></entry>
  </interpolation>
  <center>
   <x-m>-4.74</x-m>
   <y-m>0</y-m>
   <z-m>-0.24</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NGearDoorLeft</object-name>
  <property>/gear/gear/position-norm</property>
  <interpolation>
    <entry><ind>0</ind><dep>90</dep></entry>
    <entry><ind>0.1</ind><dep>0</dep></entry>
    <entry><ind>1</ind><dep>0</dep></entry>
  </interpolation>
  <center>
   <x-m>-4.30</x-m>
   <y-m>-0.21</y-m>
   <z-m>-0.77</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NGearDoorRight</object-name>
  <property>/gear/gear/position-norm</property>
  <interpolation>
    <entry><ind>0</ind><dep>90</dep></entry>
    <entry><ind>0.1</ind><dep>0</dep></entry>
    <entry><ind>1</ind><dep>0</dep></entry>
  </interpolation>
  <center>
   <x-m>-4.30</x-m>
   <y-m>0.21</y-m>
   <z-m>-0.77</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>LMainGearStrut</object-name>
  <object-name>LMainGearDoor-upper</object-name>
  <object-name>LMainGearDoor-lower</object-name>
  <object-name>LTire</object-name>
  <property>/gear/gear/position-norm</property>
  <interpolation>
    <entry><ind>0</ind><dep>80</dep></entry>
    <entry><ind>0.1</ind><dep>80</dep></entry>
    <entry><ind>1</ind><dep>0</dep></entry>
  </interpolation>
  <center>
   <x-m> 0.74055</x-m>
   <y-m>-2.7402</y-m>
   <z-m> 0.30329</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0.4</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>RMainGearStrut</object-name>
  <object-name>RMainGearDoor-upper</object-name>
  <object-name>RMainGearDoor-lower</object-name>
  <object-name>RTire</object-name>
  <property>/gear/gear/position-norm</property>
  <interpolation>
    <entry><ind>0</ind><dep>80</dep></entry>
    <entry><ind>0.1</ind><dep>80</dep></entry>
    <entry><ind>1</ind><dep>0</dep></entry>
  </interpolation>
  <center>
   <x-m> 0.74055</x-m>
   <y-m> 2.7402</y-m>
   <z-m> 0.30329</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0.4</y>
   <z>0</z>
  </axis>
 </animation>

